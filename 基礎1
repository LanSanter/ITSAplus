#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    int input = 0;
    char num[10][5][5] = {
        {{'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}},
        {{' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}},
        {{'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', ' '}, {'*', '*', '*', '*', '*'}},
        {{'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}},
        {{'*', ' ', ' ', ' ', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}},
        {{'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', ' '}, {'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}},
        {{'*', ' ', ' ', ' ', ' '}, {'*', ' ', ' ', ' ', ' '}, {'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}},
        {{'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}},
        {{'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}},
        {{'*', '*', '*', '*', '*'}, {'*', ' ', ' ', ' ', '*'}, {'*', '*', '*', '*', '*'}, {' ', ' ', ' ', ' ', '*'}, {' ', ' ', ' ', ' ', '*'}}
    };

    while (scanf("%d", &input) != EOF) {
        for (int i = 0; i < 5; i++) {
            int output = input;
            int w = 1000;

            for (int k = 0; k < 4; k++) {
                int number = output / w;
                output %= w;
                w /= 10;

                for (int j = 0; j < 5; j++) {
                    printf("%c", num[number][i][j]);
                }
                if (k != 3) printf(" ");
            }
            printf("\n");
        }
    }

    return 0;
}
